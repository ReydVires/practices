window.WML.namespace('OptionsScreen', window.WML.Screen, (function (wml) {'use strict';    var gameObject = null,        thisObject = null,        woodBg = null,        optionsTitle = null,        mainMenuButton = null,        moveCardsText = null,        moveCardsCheckBox = null,        showArrowsText = null,        showArrowsCheckBox = null,        muteOnOffButton = null,        soundText = null;    return {        init: function () {            thisObject = this;            gameObject = wml.gameObject;            thisObject.screenObjects = gameObject.add.group();        },        create: function () {            woodBg = gameObject.add.sprite(0, 0, wml.ImageAssetKeys.GUIDES_AND_BACKGROUNDS_ATLAS, 'woodBg.jpg');            thisObject.screenObjects.add(woodBg);            optionsTitle = gameObject.add.sprite(235, 15, wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'optionsTitle.png');            thisObject.screenObjects.add(optionsTitle);            mainMenuButton =                wml.GoToScreen.Button(wml.MainMenuScreen.KEY, thisObject.screenObjects, 235, 430, wml.ImageAssetKeys.STANDARD_BUTTONS_SHEET, thisObject,                                        null, null, null, null, 3, 2, 2, 3);            thisObject.screenObjects.add(mainMenuButton);            moveCardsText = gameObject.add.text(150, 100, 'Move cards ', wml.Utility.getTextStyle20());            thisObject.screenObjects.add(moveCardsText);            moveCardsCheckBox = gameObject.add.sprite(400, 95, wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'checkBoxOn.png');            moveCardsCheckBox.inputEnabled = true;            moveCardsCheckBox.events.onInputDown.add(function () {                var wmlObject = this;                    if (wmlObject.getMoveCards()) {                        wmlObject.disableMoveCards();                        moveCardsCheckBox.loadTexture(wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'checkBoxOff.png');                    } else {                        wmlObject.enableMoveCards();                        moveCardsCheckBox.loadTexture(wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'checkBoxOn.png');                    }            }, wml);            thisObject.screenObjects.add(moveCardsCheckBox);            if (wml.getMoveCards()) {                moveCardsCheckBox.loadTexture(wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'checkBoxOn.png');            } else {                moveCardsCheckBox.loadTexture(wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'checkBoxOff.png');            }            showArrowsText = gameObject.add.text(150, 150, 'Show arrows ', wml.Utility.getTextStyle20());            thisObject.screenObjects.add(showArrowsText);            showArrowsCheckBox = gameObject.add.sprite(400, 145, wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'checkBoxOn.png');            showArrowsCheckBox.inputEnabled = true;            showArrowsCheckBox.events.onInputDown.add(function () {                var wmlObject = this;                    if (wmlObject.getShowArrows()) {                        wmlObject.disableShowArrows();                        showArrowsCheckBox.loadTexture(wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'checkBoxOff.png');                    } else {                        wmlObject.enableShowArrows();                        showArrowsCheckBox.loadTexture(wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'checkBoxOn.png');                    }            }, wml);            thisObject.screenObjects.add(showArrowsCheckBox);            if (wml.getShowArrows()) {                showArrowsCheckBox.loadTexture(wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'checkBoxOn.png');            } else {                showArrowsCheckBox.loadTexture(wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'checkBoxOff.png');            }            soundText = gameObject.add.text(150, 200, 'Sound ', wml.Utility.getTextStyle20());            thisObject.screenObjects.add(soundText);            muteOnOffButton = gameObject.add.sprite(400, 195, wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'muteOff.png');            muteOnOffButton.inputEnabled = true;            muteOnOffButton.events.onInputDown.add(thisObject.muteOnOffSound, wml);            thisObject.screenObjects.add(muteOnOffButton);                                                                    if (gameObject.sound.mute) {                muteOnOffButton.loadTexture(wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'muteOff.png');            } else {                muteOnOffButton.loadTexture(wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'muteOn.png');            }            wml.fadeInGroup(thisObject.screenObjects);        },        muteOnOffSound: function () {            var wmlObject = this,                gameObject = wmlObject.gameObject;            if (gameObject.sound.mute) {                muteOnOffButton.loadTexture(wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'muteOn.png');                gameObject.sound.mute = false;            } else {                muteOnOffButton.loadTexture(wml.ImageAssetKeys.OTHER_GUI_ASSETS_ATLAS, 'muteOff.png');                gameObject.sound.mute = true;            }        },    };    }(window.WML)));